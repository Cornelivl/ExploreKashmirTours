{
  "buildCommand": "turbo build",
  "installCommand": "pnpm install",
  "outputDirectory": "apps/web/dist",
  "rewrites": [
    { 
      "source": "/api/:path*",
      "destination": "https://naserkashmirtours-api.vercel.app/:path*" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/" 
    }
  ]
} 